{
  "name": "frontend",
  "homepage": "https://orders-manager.netlify.com/",
  "description": "Frontend of app for managing orders in online store",
  "repository": {
    "type": "git",
    "url": "https://github.com/DimaDK24/orders-manager.git"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@reach/router": "^1.2.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "formik": "^2.0.10",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.9.0",
    "react-phone-input-2": "^2.11.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "remove-trailing-separator": "^1.1.0",
    "spinkit": "^2.0.1"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-redux": "^3.0.1",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-prettier": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,jsx,json,css,yml}\"",
    "lint:js": "eslint \"src/**/*.{js,jsx}\" \".storybook/**/*.js\" --max-warnings=0",
    "lint:styles": "stylelint \"src/**/*.{css,sass,scss}\"",
    "lint:code": "yarn lint:js && yarn lint:styles",
    "lint:commits": "commitlint --from origin/master --to HEAD",
    "lint": "yarn lint:code && yarn lint:commits",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react"
  ],
  "license": "MIT"
